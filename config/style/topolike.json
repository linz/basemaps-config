{
  "version": 8,
  "name": "topolike",
  "metadata": {"maputnik:renderer": "mbgljs"},
  "sources": {
    "LINZ Basemaps": {
      "type": "vector",
      "url": "https://vector.tiles.linz.io/data/topo.json"
    },
    "openmaptiles": {
      "type": "vector",
      "url": "https://api.maptiler.com/tiles/v3/tiles.json?key={key}"
    }
  },
  "sprite": "https://nst-guide.github.io/osm-liberty-topo/sprites/osm-liberty-topo",
  "glyphs": "https://orangemug.github.io/font-glyphs/glyphs/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "Background",
      "type": "background",
      "minzoom": 0,
      "filter": ["all"],
      "layout": {"visibility": "visible"},
      "paint": {"background-color": "rgba(206, 229, 242, 1)"}
    },
    {
      "id": "Coastline",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "Coastline",
      "minzoom": 0,
      "maxzoom": 24,
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "rgba(235, 238, 234, 1)",
        "fill-outline-color": "rgba(9, 132, 186, 1)",
        "fill-antialias": true,
        "fill-opacity": 1,
        "fill-translate-anchor": "map"
      }
    },
    {
      "id": "Coastline2",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "coastline",
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": {
          "stops": [
            [0, "rgba(30, 133, 30, 1)"],
            [9.99, "rgba(202, 226, 202, 1)"],
            [10, "rgba(232, 232, 232, 1)"],
            [20, "rgba(232, 232, 232, 1)"]
          ]
        },
        "fill-antialias": true
      }
    },
    {
      "id": "Vegetation-scattscrub",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "landcover",
      "minzoom": 10,
      "filter": ["all", ["==", "distribution", "scattered"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": {
          "stops": [
            [10, "rgba(148, 199, 111, 0.2)"],
            [24, "rgba(148, 199, 111, 0.40)"]
          ]
        },
        "fill-antialias": false
      }
    },
    {
      "id": "Vegetation-scrub",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "landcover",
      "minzoom": 10,
      "filter": ["all", ["==", "natural", "scrub"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": {
          "stops": [
            [10, "rgba(148, 199, 111, 0.4)"],
            [24, "rgba(148, 199, 111, 0.80)"]
          ]
        }
      }
    },
    {
      "id": "Vegetation-exotic",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "landcover",
      "minzoom": 10,
      "filter": ["all", ["==", "tree", "exotic"], ["==", "landuse", "wood"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": {
          "stops": [
            [10, "rgba(115, 181, 131, 0.4)"],
            [24, "rgba(115, 181, 131, 0.8)"]
          ]
        }
      }
    },
    {
      "id": "Vegetation-native",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "landcover",
      "minzoom": 10,
      "filter": ["all", ["==", "tree", "native"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": {
          "stops": [
            [10, "rgba(121, 160, 72, 0.4)"],
            [24, "rgba(121, 160, 72, 0.8)"]
          ]
        }
      }
    },
    {
      "id": "pattern-vineyard",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "landcover",
      "minzoom": 10,
      "maxzoom": 24,
      "filter": ["all", ["==", "landuse", "vineyard"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "rgba(27, 127, 36, 0.1)",
        "fill-outline-color": "rgba(255, 255, 255, 1)"
      }
    },
    {
      "id": "pattern-orchard",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "landcover",
      "minzoom": 10,
      "maxzoom": 24,
      "filter": ["all", ["==", "landuse", "orchard"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "rgba(27, 127, 36, 0.1)",
        "fill-outline-color": "rgba(255, 255, 255, 1)"
      }
    },
    {
      "id": "Fence-lines",
      "type": "line",
      "source": "LINZ Basemaps",
      "source-layer": "landcover",
      "minzoom": 14,
      "filter": [
        "all",
        ["==", "barrier", "fence"],
        ["==", "class", "farmland"]
      ],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": {
          "stops": [
            [15, "rgba(100, 100, 100, 0.4)"],
            [19, "rgba(100, 100, 100, 0.8)"]
          ]
        },
        "line-width": 1
      }
    },
    {
      "id": "Fence-posts",
      "type": "line",
      "source": "LINZ Basemaps",
      "source-layer": "landcover",
      "minzoom": 16.5,
      "filter": [
        "all",
        ["==", "barrier", "fence"],
        ["==", "class", "farmland"]
      ],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": {
          "stops": [
            [15, "rgba(160, 90, 26, 0.4)"],
            [19, "rgba(160, 90, 26, 0.6)"]
          ]
        },
        "line-width": 1.5,
        "line-dasharray": [1, 7],
        "line-offset": 1
      }
    },
    {
      "id": "Vegetation-lines",
      "type": "line",
      "source": "LINZ Basemaps",
      "source-layer": "landcover",
      "minzoom": 14,
      "filter": ["all", ["==", "class", "wood"], ["==", "natural", "tree_row"]],
      "layout": {"visibility": "visible"},
      "paint": {"line-color": "rgba(148, 199, 111, 0.75)", "line-width": 4}
    },
    {
      "id": "Coastline-Line",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "coastline",
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-outline-color": "rgba(9, 132, 186, 1)",
        "fill-antialias": true,
        "fill-color": "rgba(0, 0, 0, 0)"
      }
    },
    {
      "id": "contours-all",
      "type": "line",
      "source": "LINZ Basemaps",
      "source-layer": "contours",
      "minzoom": 9,
      "maxzoom": 23,
      "filter": [
        "all",
        ["!has", "designated"],
        ["!=", "type", "index"],
        ["!has", "nat_form"]
      ],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "rgba(206, 103, 6, 0.75)",
        "line-width": {"stops": [[12, 0.4], [16, 0.2]]}
      }
    },
    {
      "id": "contours-natural",
      "type": "line",
      "source": "LINZ Basemaps",
      "source-layer": "contours",
      "minzoom": 9,
      "maxzoom": 23,
      "filter": ["all", ["has", "nat_form"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "rgba(206, 103, 6, 0.75)",
        "line-width": 0.6,
        "line-dasharray": [30, 10, 10, 10]
      }
    },
    {
      "id": "contours-designated",
      "type": "line",
      "source": "LINZ Basemaps",
      "source-layer": "contours",
      "minzoom": 9,
      "maxzoom": 23,
      "filter": ["all", ["has", "designated"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "rgba(206, 103, 6, 0.75)",
        "line-width": 0.4,
        "line-dasharray": [30, 30]
      }
    },
    {
      "id": "contours-index",
      "type": "line",
      "source": "LINZ Basemaps",
      "source-layer": "contours",
      "minzoom": 9,
      "maxzoom": 23,
      "filter": ["all", ["==", "type", "index"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": {
          "stops": [
            [10, "rgba(205, 137, 54, 0.2)"],
            [13, "rgba(207, 129, 43, 0.75)"]
          ]
        },
        "line-width": {"stops": [[9, 0.2], [12, 0.3], [15, 1]]}
      }
    },
    {
      "id": "countourlabels",
      "type": "symbol",
      "source": "LINZ Basemaps",
      "source-layer": "contours",
      "maxzoom": 16,
      "filter": ["any", ["==", "type", "index"], ["has", "designated"]],
      "layout": {
        "symbol-placement": "line",
        "text-field": "{elevation}",
        "visibility": "visible",
        "text-size": {"stops": [[13, 8], [15, 12]]},
        "text-line-height": 1,
        "text-padding": 0,
        "text-allow-overlap": false,
        "text-font": ["Open Sans Bold"],
        "text-keep-upright": false,
        "symbol-spacing": {"stops": [[10, 150], [16, 300]]}
      },
      "paint": {
        "text-color": "rgba(193, 104, 9, 1)",
        "text-halo-color": "rgba(243, 243, 242, 1)",
        "text-halo-width": 1,
        "text-halo-blur": 0.5
      }
    },
    {
      "id": "Pacels",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "parcel_boundaries",
      "minzoom": 16,
      "maxzoom": 24,
      "filter": [
        "none",
        ["==", "parcel_intent", "Road"],
        ["==", "parcel_intent", "Hydro"]
      ],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "rgba(0, 0, 0, 0)",
        "fill-outline-color": {
          "stops": [
            [16, "rgba(220, 220, 220, 1)"],
            [24, "rgba(147, 147, 147, 1)"]
          ]
        },
        "fill-antialias": true,
        "fill-opacity": 1
      }
    },
    {
      "id": "Aeroway-airstrip-sealed",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "aeroway",
      "minzoom": 13,
      "filter": ["all", ["==", "class", "runway"], ["==", "surface", "sealed"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "rgba(193, 193, 193, 0.5)",
        "fill-outline-color": "rgba(36, 36, 36, 0)",
        "fill-antialias": false
      }
    },
    {
      "id": "Aeroway-airstrip-grass",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "aeroway",
      "minzoom": 13,
      "filter": ["all", ["==", "class", "runway"], ["!has", "surface"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "rgba(96, 197, 111, 0.5)",
        "fill-outline-color": "rgba(36, 36, 36, 0)",
        "fill-antialias": false
      }
    },
    {
      "id": "Aeroway-runway",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "aeroway",
      "minzoom": 13,
      "filter": ["all", ["==", "class", "runway"], ["==", "surface", "sealed"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-outline-color": "rgba(36, 36, 36, 0.5)",
        "fill-color": "rgba(0, 0, 0, 0)",
        "fill-antialias": true
      }
    },
    {
      "id": "Aeroway-airport",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "aeroway",
      "minzoom": 10,
      "filter": ["all", ["==", "class", "aerodrome"]],
      "layout": {"visibility": "visible"},
      "paint": {"fill-color": "rgba(189, 189, 189, 0.5)"}
    },
    {
      "id": "Aeroway-airstrip-lines",
      "type": "line",
      "source": "LINZ Basemaps",
      "source-layer": "aeroway",
      "minzoom": 13,
      "filter": ["all", ["==", "class", "runway"], ["!has", "surface"]],
      "layout": {"visibility": "visible"},
      "paint": {"line-color": "rgba(36, 36, 36, 0.5)", "line-dasharray": [5, 5]}
    },
    {
      "id": "Aeroway",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "aeroway",
      "minzoom": 10,
      "layout": {"visibility": "none"},
      "paint": {"fill-color": "rgba(189, 189, 189, 0.5)"}
    },
    {
      "id": "Water-DrainCanal-Lines",
      "type": "line",
      "source": "LINZ Basemaps",
      "source-layer": "water",
      "minzoom": 10,
      "maxzoom": 21,
      "filter": [
        "any",
        ["==", "feat_type", "drain_cl"],
        ["==", "feat_type", "canal_cl"]
      ],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "rgba(0, 140, 204, 1)",
        "line-opacity": 1,
        "line-width": 0.5
      }
    },
    {
      "id": "Water-River-Lines",
      "type": "line",
      "source": "LINZ Basemaps",
      "source-layer": "water",
      "minzoom": 13,
      "maxzoom": 21,
      "filter": ["all", ["==", "feat_type", "river_cl"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "rgba(0, 140, 204, 1)",
        "line-opacity": 1,
        "line-width": 0.75
      }
    },
    {
      "id": "Water-River-Lines-Named",
      "type": "line",
      "source": "LINZ Basemaps",
      "source-layer": "water",
      "minzoom": 8,
      "maxzoom": 13,
      "filter": ["all", ["==", "feat_type", "river_cl"], ["has", "name"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "rgba(0, 140, 204, 1)",
        "line-opacity": 1,
        "line-width": 0.75
      }
    },
    {
      "id": "Water-Rivers-Poly",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "water",
      "minzoom": 13,
      "maxzoom": 21,
      "filter": [
        "any",
        ["==", "feat_type", "river_poly"],
        ["==", "feat_type", "canal_poly"]
      ],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "rgba(204, 232, 245, 1)",
        "fill-outline-color": "rgba(0, 140, 204, 1)",
        "fill-opacity": 1,
        "fill-translate-anchor": "map",
        "fill-antialias": true
      }
    },
    {
      "id": "Water-Lakes-named",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "water",
      "minzoom": 8,
      "maxzoom": 13,
      "filter": ["all", ["==", "class", "lake"], ["has", "name"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "rgba(204, 232, 245, 1)",
        "fill-outline-color": "rgba(0, 140, 204, 1)",
        "fill-opacity": 1,
        "fill-translate-anchor": "map",
        "fill-antialias": true
      }
    },
    {
      "id": "Water-Rivers-Poly-Named",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "water",
      "minzoom": 8,
      "maxzoom": 13,
      "filter": ["all", ["==", "feat_type", "river_poly"], ["has", "name"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "rgba(204, 232, 245, 1)",
        "fill-outline-color": "rgba(0, 140, 204, 1)",
        "fill-opacity": 1,
        "fill-translate-anchor": "map",
        "fill-antialias": true
      }
    },
    {
      "id": "Water-Canal-Poly-Named",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "water",
      "minzoom": 8,
      "maxzoom": 13,
      "filter": ["all", ["==", "feat_type", "canal_poly"], ["has", "name"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "rgba(204, 232, 245, 1)",
        "fill-outline-color": "rgba(0, 140, 204, 1)",
        "fill-opacity": 1,
        "fill-translate-anchor": "map",
        "fill-antialias": true
      }
    },
    {
      "id": "Water-Lakes",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "water",
      "minzoom": 0,
      "maxzoom": 21,
      "filter": ["all", ["==", "class", "lake"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "rgba(204, 232, 245, 1)",
        "fill-outline-color": "rgba(0, 140, 204, 1)",
        "fill-opacity": 1,
        "fill-translate-anchor": "map",
        "fill-antialias": true
      }
    },
    {
      "id": "Landuse-Residential",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "landuse",
      "maxzoom": 15,
      "filter": ["all", ["==", "class", "residential"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": {
          "stops": [
            [1, "rgba(164, 164, 164, 1)"],
            [10, "rgba(164, 164, 164, 0.95)"],
            [16, "rgba(164, 164, 164, 0.75)"]
          ]
        }
      }
    },
    {
      "id": "Poi-Tank-Pt-Background",
      "type": "circle",
      "source": "LINZ Basemaps",
      "source-layer": "poi",
      "maxzoom": 15,
      "filter": ["all", ["==", "class", "storage_tank_pt"]],
      "layout": {"visibility": "visible"},
      "paint": {"circle-radius": {"stops": [[12, 2], [15, 5]]}}
    },
    {
      "id": "Poi-Tank-Pt-Fill-Empty",
      "type": "circle",
      "source": "LINZ Basemaps",
      "source-layer": "poi",
      "maxzoom": 15,
      "filter": ["all", ["==", "class", "storage_tank_pt"]],
      "layout": {"visibility": "visible"},
      "paint": {
        "circle-color": "rgba(255, 255, 255, 1)",
        "circle-radius": {"stops": [[12, 1.5], [15, 3.5]]}
      }
    },
    {
      "id": "Poi-Tank-Pt-Fill-Fuel",
      "type": "circle",
      "source": "LINZ Basemaps",
      "source-layer": "poi",
      "maxzoom": 15,
      "filter": [
        "all",
        ["==", "class", "storage_tank_pt"],
        ["==", "store_item", "fuel"]
      ],
      "layout": {"visibility": "visible"},
      "paint": {
        "circle-color": "rgba(0, 0, 0, 1)",
        "circle-radius": {"stops": [[12, 1.5], [14, 3.5]]}
      }
    },
    {
      "id": "Poi-Tank-Pt-Fill-Water",
      "type": "circle",
      "source": "LINZ Basemaps",
      "source-layer": "poi",
      "maxzoom": 15,
      "filter": [
        "all",
        ["==", "class", "storage_tank_pt"],
        ["==", "store_item", "water"]
      ],
      "layout": {"visibility": "visible"},
      "paint": {
        "circle-color": "rgba(110, 167, 205, 1)",
        "circle-radius": {"stops": [[12, 1.5], [15, 3.5]]}
      }
    },
    {
      "id": "buildings-shadow",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "building",
      "minzoom": 17,
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": {
          "stops": [
            [15, "rgba(199, 199, 199, 0.75)"],
            [19, "rgba(125, 125, 125, 1)"]
          ]
        },
        "fill-antialias": true,
        "fill-opacity": 1,
        "fill-translate": {"base": 1, "stops": [[17, [1, 1]], [19, [3, 3]]]}
      }
    },
    {
      "id": "buildings",
      "type": "fill",
      "source": "LINZ Basemaps",
      "source-layer": "building",
      "minzoom": 15,
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": {
          "stops": [
            [15, "rgba(148, 148, 148, 1)"],
            [19, "rgba(220, 220, 220, 1)"]
          ]
        },
        "fill-antialias": true,
        "fill-opacity": 1
      }
    },
    {
      "id": "buildings-line",
      "type": "line",
      "source": "LINZ Basemaps",
      "source-layer": "building",
      "minzoom": 18,
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "rgba(152, 145, 145,0.75)",
        "line-width": {"stops": [[17, 0.5], [24, 1]]}
      }
    },
    {
      "id": "height-point",
      "type": "circle",
      "source": "LINZ Basemaps",
      "source-layer": "contours",
      "minzoom": 13,
      "maxzoom": 16,
      "filter": ["all", ["==", "class", "peak"]],
      "layout": {"visibility": "visible"},
      "paint": {"circle-color": "rgba(47, 47, 46, 1)", "circle-radius": 2}
    },
    {
      "id": "Transport-FootTracks-test",
      "type": "line",
      "source": "LINZ Basemaps",
      "source-layer": "transportation",
      "minzoom": 14,
      "maxzoom": 24,
      "filter": ["all", ["==", "class", "track"], ["==", "track_use", "foot"]],
      "layout": {
        "visibility": "visible",
        "line-cap": "butt",
        "line-join": "bevel"
      },
      "paint": {
        "line-color": "rgba(78, 78, 78, 0.8)",
        "line-width": {"stops": [[14, 0.5], [15, 1.5], [19, 3]]},
        "line-dasharray": {
          "base": 1,
          "stops": [[14, [1, 4]], [15, [2, 3]], [16, [2.5, 3]]]
        }
      }
    },
    {
      "id": "Transport-FootTracks",
      "type": "line",
      "source": "LINZ Basemaps",
      "source-layer": "transportation",
      "minzoom": 14,
      "maxzoom": 24,
      "filter": ["all", ["==", "class", "track"], ["==", "track_use", "foot"]],
      "layout": {
        "visibility": "none",
        "line-cap": "butt",
        "line-join": "bevel"
      },
